{
  "name": "@idesigncode/date-input",
  "version": "1.0.0",
  "description": "An ESM date input component with a composable datepicker UI for React.",
  "keywords": [
    "react",
    "date",
    "input",
    "calendar",
    "datepicker",
    "composable",
    "esm",
    "date-fns",
    "mjs"
  ],
  "license": "MIT",
  "author": "Matthew Nuthall",
  "repository": "github:idesigncode/date-input",
  "bugs": "https://github.com/idesigncode/date-input/issues",
  "homepage": "https://github.com/idesigncode/date-input#readme",
  "engines": {
    "node": ">= 18.0.0"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions"
  ],
  "scripts": {
    "eslint": "eslint . --ext .js,.cjs,.mjs --fix --ignore-path .gitignore",
    "prepare": "npm run prepare:clean && npm run prepare:css && npm run prepare:js && husky install",
    "prepare:clean": "node .npm-prepare-clean.cjs",
    "prepare:css": "sass --no-source-map src:. && postcss *.css -d .",
    "prepare:js": "babel src --out-dir . --ignore **/*.test.mjs,**/*.stories.mjs --keep-file-extension",
    "prepublishOnly": "npm run test",
    "prettier": "prettier --write . --ignore-path .gitignore",
    "storybook": "storybook dev --ci -p 6006",
    "storybook:build": "storybook build --quiet --docs",
    "storybook:start": "npx http-server ./storybook-static -p 6006",
    "test": "npm run test:prettier && npm run test:eslint",
    "test:eslint": "eslint . --ext .js,.cjs,.mjs --ignore-path .gitignore",
    "test:prettier": "prettier --check . --ignore-path .gitignore",
    "test:storybook": "test-storybook --stories-json --coverage",
    "test:storybook:update": "npm run test:storybook -- --updateSnapshot"
  },
  "files": [
    "helpers",
    "hooks",
    "icons",
    "layout.css",
    "theme.css",
    "theme.dark.css",
    "theme.light.css",
    "ArrowButton.mjs",
    "Calendar.css",
    "Calendar.mjs",
    "CalendarArrow.css",
    "CalendarArrow.mjs",
    "CalendarContext.mjs",
    "CalendarTabs.css",
    "CalendarTabs.mjs",
    "DateInput.css",
    "DateInput.mjs",
    "DateInputContext.mjs",
    "Field.css",
    "Field.mjs",
    "View.css",
    "View.mjs",
    "ViewBody.css",
    "ViewBody.mjs",
    "ViewContext.mjs",
    "ViewHead.css",
    "ViewHead.mjs",
    "ViewWeekDays.css",
    "ViewWeekDays.mjs"
  ],
  "exports": {
    "./Calendar.css": "./Calendar.css",
    "./Calendar.mjs": "./Calendar.mjs",
    "./CalendarArrow.css": "./CalendarArrow.css",
    "./CalendarArrow.mjs": "./CalendarArrow.mjs",
    "./CalendarTabs.css": "./CalendarTabs.css",
    "./CalendarTabs.mjs": "./CalendarTabs.mjs",
    "./DateInput.css": "./DateInput.css",
    "./DateInput.mjs": "./DateInput.mjs",
    "./Field.css": "./Field.css",
    "./Field.mjs": "./Field.mjs",
    "./View.css": "./View.css",
    "./View.mjs": "./View.mjs",
    "./ViewBody.css": "./ViewBody.css",
    "./ViewBody.mjs": "./ViewBody.mjs",
    "./ViewHead.css": "./ViewHead.css",
    "./ViewHead.mjs": "./ViewHead.mjs",
    "./ViewWeekDays.css": "./ViewWeekDays.css",
    "./ViewWeekDays.mjs": "./ViewWeekDays.mjs",
    "./layout.css": "./layout.css",
    "./package.json": "./package.json",
    "./theme.css": "./theme.css",
    "./theme.dark.css": "./theme.dark.css",
    "./theme.light.css": "./theme.light.css"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@idesigncode/storybook-tools": "^3.0.1",
    "@storybook/addon-coverage": "^0.0.9",
    "@storybook/addon-docs": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addons": "^7.1.1",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.1.1",
    "@storybook/react-webpack5": "^7.1.1",
    "@storybook/test-runner": "^0.12.0",
    "@storybook/testing-library": "^0.2.0",
    "autoprefixer": "^10.4.14",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "cssnano": "^6.0.1",
    "eslint": "^8.46.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "jest-image-snapshot": "^6.2.0",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.64.1",
    "storybook": "^7.1.1",
    "storybook-dark-mode": "^3.0.1",
    "timeshift": "^0.0.2"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
